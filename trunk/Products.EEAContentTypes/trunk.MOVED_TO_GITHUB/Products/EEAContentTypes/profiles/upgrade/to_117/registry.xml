<registry>
    <record name="Products.EEAContentTypes.browser.interfaces.IEEAContentTypesSettings.fullwidthFor" interface="Products.EEAContentTypes.browser.interfaces.IEEAContentTypesSettings" field="fullwidthFor">
        <field type="plone.registry.field.Tuple">
            <default>
                <element>GIS Application</element>
            </default>
            <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="eea" ns0:translate="">Enable body fullwidth class for the following content-types</description>
            <required>False</required>
            <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="eea" ns0:translate="">Fullwidth content types</title>
            <value_type type="plone.registry.field.Choice">
                <vocabulary>plone.app.vocabularies.ReallyUserFriendlyTypes</vocabulary>
            </value_type>
        </field>
        <value>
            <element>GIS Application</element>
        </value>
    </record>
</registry>
