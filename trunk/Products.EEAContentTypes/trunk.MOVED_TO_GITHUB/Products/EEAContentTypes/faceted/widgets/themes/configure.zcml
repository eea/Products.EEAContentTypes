  xmlns:faceted="http://namespaces.zope.org/faceted"
  i18n_domain="eea.faceted">

  <faceted:widget factory=".widget.Widget" />

  <browser:resource
    name="Products.EEAContentTypes.faceted.themes.view.js"
    file="./view.js"
    />

  <browser:resource
    name="Products.EEAContentTypes.faceted.themes.edit.js"
    file="./edit.js"
    />

  <browser:resource
    name="Products.EEAContentTypes.faceted.themes.view.css"
    file="./view.css"
    />

  <browser:resource
    name="Products.EEAContentTypes.faceted.themes.edit.css"
    file="./edit.css"
    />

</configure>
