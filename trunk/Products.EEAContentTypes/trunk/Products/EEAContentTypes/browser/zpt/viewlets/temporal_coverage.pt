<!-- Temporal coverage -->
<tal:block metal:define-macro="temp-coverage"
        tal:condition="view/available">
    <tal:render tal:define="value context/@@formatTempCoverage" tal:condition="value">
            <span id="temporal_dynamic" i18n:translate="" class="temporal-coverage">Dynamic</span>
            <dt class="temporal-coverage">
                <metal:basic use-macro="here/dataset_view/macros/tooltip" />
                <span i18n:translate="">Temporal coverage:</span>
            </dt>
            <dd class="temporal-coverage" tal:content="value" id="tempCoverage"></dd>
    </tal:render>
</tal:block>
