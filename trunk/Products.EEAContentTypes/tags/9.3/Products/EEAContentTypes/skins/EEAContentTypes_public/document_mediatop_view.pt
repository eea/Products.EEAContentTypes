<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  lang="en"
  metal:use-macro="context/main_template/macros/master"
  i18n:domain="eea">
  <body>

    <metal:content-core fill-slot="content-core">
      <metal:content-core define-macro="content-core">

        <metal:document use-macro="here/document_macros/macros/document">
          <tal:document metal:fill-slot="related_multimedia">

            <div metal:use-macro="here/document_relateditems/macros/multimedia_top_videos">
              show related multimedia videos if they exist
            </div>

          </tal:document>
        </metal:document>

      </metal:content-core>
    </metal:content-core>

  </body>
</html>
