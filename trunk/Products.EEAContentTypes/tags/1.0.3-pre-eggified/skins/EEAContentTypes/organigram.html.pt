border: 1px solid #888888;
padding: 10px;
width:250px !important;
font-size:8px;
height: 100px !important;
font-family:Verdana,sans-serif;
font-size-adjust:none;
font-stretch:normal;
font-style:normal;
font-variant:normal;
font-weight:normal;
line-height:normal;
text-align:left;
}

* html .org_container {
width: 90px !important;
padding: 0 !important;
margin: 0 !important;
}

:first-child+html .org_container {
width: 90px !important;
}

.org_container {
float: left;
width: 100px !important;
}

.org_head {
position: relative;
border:1px solid #888888;
background-color:#CCCCCC;
height:135px !important;
width:80px !important;
margin:0 10px 0 10px;
padding:0 3px 0 3px;
font-family:Verdana,sans-serif;
font-size:7px;
font-size-adjust:none;
font-stretch:normal;
font-style:normal;
font-variant:normal;
font-weight:normal;
line-height:normal;
text-align:left;
}
*:first-child+html .org_head {
width: 65px !important;
}
* html .org_head {
width: 55px !important;
}
* html  .org_subhead {
margin: 5px 0 0 0 !important;
width: 60px !important;
}

*:first-child+html .org_subhead {
width: 60px !important;
margin:10px 0 0 0 !important;
}

.org_subhead {
border:1px solid #888888;
background-color:#EFEFEF;
margin:10px 0 0 23px;
padding:6px 3px 0 3px;
height:128px !important;
width:75px !important;
font-family:Verdana,sans-serif;
font-size:7px;
font-size-adjust:none;
font-stretch:normal;
font-style:normal;
font-variant:normal;
font-weight:normal;
line-height:normal;
text-align:left;
}
.org_staff_number {
/*
position: relative;
bottom:0;
right:0;
*/
}
.org_staff_total {
/* bottom: 1em; */
padding-bottom: 2px;
}

.org_line {
height:20px !important;
margin: 15px 0 0 0;
padding:0;
border-top: 1px solid black;
font-size:10px;
font-family:Verdana,sans-serif;
font-size-adjust:none;
font-stretch:normal;
font-style:normal;
font-variant:normal;
font-weight:normal;
line-height:normal;
text-align:left;
}

* html  .org_line_first {
margin:0pt 0pt 0pt 12px;
/* Remove the comment below for non custom EDO first */
/* margin:0pt 0pt 0pt 56px; */
}
:first-child+html .org_line {
margin: 17px 0 0 0;
}
:first-child+html .org_line_first {
margin:0pt 0pt 0pt 15px;
/* Remove the comment below for non custom EDO first */
/* margin:0pt 0pt 0pt 56px; */
}

.org_line_first {
border-left:1px solid black;
margin:0pt 0pt 0pt 20px;
height:150px !important;
/* Remove the comment below for non custom EDO first */
/* margin:0pt 0pt 0pt 56px; */
}

.org_line_upper {
border-left:1px solid black;
height:15px !important;
margin-left:56px;
}

.org_line_last {
width: 57px !important;
}

.org_line div {
border-right: 1px solid black;
height:20px !important;
width: 56px !important;
}

* html .org_child {
margin: 0 0 0 6px !important;
width: 2px !important;
}
* html .org_child1 {
page-break-after: always;
}
* html .org_child2 {
position: relative;
right: 9px;

}
* html div.org_child3 {
margin: 0 !important;
position: relative !important;
right: 12px;
}
* html dl.org_child3 {
position: relative !important;
right: 12px;
}
* html div.org_child4 {
margin: 0 !important;
position: relative !important;
right: 15px;
}
* html dl.org_child4 {
margin: 0 !important;
position: relative;
bottom: 70px;
left: 9px;
}

:first-child+html .org_child {
margin: 0 0 0 15px !important;
height: 146px !important;
}

.org_child {
float:left;
border-left: 1px solid black;
height: 145px !important;
padding: 0 !important;
width: 2px !important;
margin: 0 0 0 20px !important;
}
* html .org_child_last {
margin: 0 0 0 6px !important;
height: 76px !important;
}
*:first-child+html .org_child_last {
height: 76px !important;
}

.org_child_last {
height: 76px !important;
}
*:first-child+html .org_child4 {
